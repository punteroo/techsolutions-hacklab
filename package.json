{
  "name": "techsolutions-security-lab",
  "version": "1.0.0",
  "description": "Educational security lab simulating TechSolutions S.A. infrastructure",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "init-db": "ts-node scripts/init-database.ts",
    "generate-logs": "ts-node scripts/generate-logs.ts",
    "check-leaks": "ts-node scripts/check-leaks.ts",
    "correlate-timeline": "ts-node scripts/correlate-timeline.ts",
    "generate-timeline": "ts-node scripts/generate-timeline.ts",
    "analyze-ransomware": "ts-node scripts/analyze-ransomware.ts",
    "simulate-recovery": "ts-node scripts/simulate-recovery.ts",
    "setup": "npm install && npm run init-db && npm run generate-logs"
  },
  "keywords": [
    "security",
    "honeypot",
    "cybersecurity",
    "education",
    "forensics"
  ],
  "author": "Security Research Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
